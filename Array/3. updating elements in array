/*CHANGING VALUES IN ARRAY*/

#include<bits/stdc++.h>
using namespace std;

int main(){
	int mathmarks[5];
	mathmarks[0] = 45;
	mathmarks[1] = 78;
	mathmarks[2] = 98;
	mathmarks[3] = 89;
	cout<<"These are mathmarks"<<endl;
	cout<<mathmarks[0]<<" ";
	cout<<mathmarks[1]<<" ";
	cout<<mathmarks[2]<<" ";
	cout<<mathmarks[3]<<endl;
	
	mathmarks[2] = 67;
	mathmarks[3] = 53;
	mathmarks[4] = 22;
	cout<<"Updated values"<<endl;
	cout<<mathmarks[0]<<" ";
	cout<<mathmarks[1]<<" ";
	cout<<mathmarks[2]<<" ";
	cout<<mathmarks[3]<<" ";
	cout<<mathmarks[4]<<" ";
	
}
