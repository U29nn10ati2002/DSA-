//Largest element of Array

#include<bits/stdc++.h>
using namespace std;

int main(){
	int n;
	cin>>n;
	
	int A[n];
	
	for(int i=0; i<n; i++){
		cin>>A[i];
		cout<<A[i]<<" ";
	}
	cout<<endl;
	
	int Largest=INT_MIN;
	for(int i=0; i<n; i++){
		if(A[i]>Largest){
			Largest = A[i];
		}
	}
	cout<<Largest;
}
