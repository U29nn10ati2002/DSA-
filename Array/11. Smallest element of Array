//Smallest element of Array

#include<bits/stdc++.h>
using namespace std;

int main(){
	int n;
	cin>>n;
	
	int A[n];
	
	cout<<"Array: ";
	for(int i=0; i<n; i++){
		cin>>A[i];
		cout<<A[i]<<" ";
	}
	cout<<endl;
	
	int Smallest=INT_MAX;
	for(int i=0; i<n; i++){
		if(A[i]<Smallest){
			Smallest = A[i];
		}
	}
	cout<<"Smallest element of Array: "<<Smallest;
}
