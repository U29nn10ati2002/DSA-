//Lenght of Array

#include<bits/stdc++.h>
using namespace std;

int Length(char input[]){
	int count=0;
	for(int i=0; input[i]!='\0'; i++){
		count++;
	}
	return count;
}

int main(){
	char name[100];
	cout<<"Enetr your name ";
	cin>>name;
	cout<<"Your name is "<<name<<endl;
	cout<<"Length "<<Length(name);
}
