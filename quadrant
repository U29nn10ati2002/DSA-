/*Write a C program to accept a coordinate point in a XY coordinate system and determine in which quadrant the coordinate point lies.*/

#include<bits/stdc++.h>
using namespace std;
int main(){
	int x,y;
	cin>>x>>y;
	if((x>=0) && (y>=0)){
		cout<<"Coordinates "<<x<<y<<" lies in the first quadrant.";
	}
	if((x<=0) && (y>=0)){
		cout<<"Coordinates "<<x<<y<<" lies in the second quadrant.";
	}
	if((x<=0) && (y<=0)){
		cout<<"Coordinates "<<x<<y<<" lies in the third quadrant.";
	}
	else{
		cout<<"Coordinates "<<x<<y<<" lies in the fourth quadrant.";
	}
}
