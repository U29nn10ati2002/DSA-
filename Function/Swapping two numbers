//Swapping using functions

#include<bits/stdc++.h>
using namespace std;
int main(){
	int a;
	int b;
	cin>>a>>b;
	cout<<"Value of a: "<<a<<endl;
	cout<<"Value of b: "<<b<<endl;
	cout<<endl;
	
	swap(a,b);
	cout<<"AFTER SWAPPING"<<endl;
	cout<<"Value of a; "<<a<<endl;
	cout<<"Value of b: "<<b<<endl;
}
