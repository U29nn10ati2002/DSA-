//Swapping two values

#include<bits/stdc++.h>
using namespace std;
int main(){
	int a;
	int b;
	cin>>a>>b;
	cout<<"Value of a: "<<a<<endl;
	cout<<"Value of b: "<<b<<endl;
	cout<<endl;
	
	int temp;
	temp=a;
	a=b;
	b=temp;
	cout<<"AFTER SWAPPING"<<endl;
	cout<<"Value of a: "<<a<<endl;
	cout<<"Value of b: "<<b<<endl;
}
