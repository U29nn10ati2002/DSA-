#include<bits/stdc++.h>
using namespace std;

int main(){
	int n;
	cin>>n;
	int A[n];
	int add;
	int multiplication;
	cout<<"Set:";
	
	for(int i=0; i<n; i++){
		cin>>A[i];
		cout<<A[i]<<" ";
	}
	cout<<endl;
	cout<<"Composition Table: Addition"<<endl;
	
	for(int i=0; i<n; i++){
		for(int j=0; j<n; j++){
			add = (i+j)%5;
			cout<<add<<" ";
		}
		cout<<endl;
	}
	cout<<endl;
	cout<<"Composition Table: Multiplication"<<endl;
	
	for(int i=0; i<n; i++){
		for(int j=0; j<n; j++){
			multiplication = (i*j)%5;
			cout<<multiplication<<" ";
		}
		cout<<endl;
	}
}
